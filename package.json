{
  "name": "@limo/eslint-config",
  "version": "1.04.13",
  "description": "ESLint rules configuration for @limo",
  "author": "aR",
  "license": "UNLICENSED",
  "private": true,
  "main": "lib/index.js",
  "types": "@types/index.d.ts",
  "scripts": {
    "tsc-w": "tsc --watch",
    "copyPackage": "cp -r ./{package.json,yarn.lock,tsdoc.json,CHANGELOG.md,README.md,.markdownlint.json} c:/npm/@limo/eslint-config/",
    "removePackage": "rm -rf c:/npm/@limo/eslint-config/{@types,lib}",
    "build": "yarn removePackage && yarn copyPackage && tsc --skipLibCheck",
    "uP": "yarn upgrade --latest"
  },
  "keywords": [
    "eslint",
    "prettier",
    "linting",
    "@limo"
  ],
  "presets": [
    "env"
  ],
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.6",
    "prettier": "^2.1.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "@limo/types": "c:/npm/@limo/types",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.6",
    "prettier": "^2.1.0",
    "typescript": "^4.0.2"
  }
}

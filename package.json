{
  "name": "@limo/eslint-config",
  "version": "1.09.02",
  "description": "Linting rules & generic configuration for @limo app.",
  "author": "Rashed Makkouk",
  "displayName": "@limo/eslint-config",
  "license": "UNLICENSED",
  "private": true,
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "tsc-w": "tsc --watch",
    "build-delete": "rm -rf ./dist/* && rm -rf ./dist/.[!.]*",
    "copy-meta": "cp -r ./{package.json,yarn.lock,README.md,LICENSE} ./src/config/{.,*} ./dist/",
    "publish-npm": "rm -rf c:/npm/@limo/eslint-config/* && rm -rf c:/npm/@limo/eslint-config/.[!.]* && cp -r ./dist/. c:/npm/@limo/eslint-config/",
    "build": "yarn build-delete && tsc --skipLibCheck && yarn copy-meta && yarn publish-npm",
    "uP": "yarn upgrade --latest"
  },
  "keywords": [
    "eslint",
    "typedoc",
    "tsdoc",
    "git",
    "md",
    "prettier",
    "linting",
    "@limo"
  ],
  "presets": [
    "env"
  ],
  "devDependencies": {
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.7",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.7",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}

      "**/docs/**",
      "**/assets/**"
    ],
    "todohighlight.include": [
      "**/*.ts",
      "**/*.tsx",
    ],
    "todohighlight.defaultStyle": {
      "color": "#fff",
      "backgroundColor": "transparent",
      "border": "1px solid",
      "borderRadius": "4px",
      "isWholeLine": false,
    },

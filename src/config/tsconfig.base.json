{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "lib": [
      "ES2017",
      "ES2018"
    ],
    "baseUrl": ".",
    "rootDir": "./src",
    "outDir": "./dist/lib",
    "declaration": false,
    "sourceMap": true,
    "removeComments": false,
    "noEmit": false,
    "isolatedModules": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmitOnError": false,
    "skipLibCheck": false,
    "types": [
      "node"
    ]
  }
}